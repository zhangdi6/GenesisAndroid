    <LinearLayout
        android:id="@+id/llCodeContent"
        android:layout_width="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="50dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titlelay">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="22dp"
            android:text="@string/code_content"
            android:textColor="@color/_333333"
            android:textSize="13sp"/>
        <TextView
            android:id="@+id/tvCodePhone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="22dp"
            android:text="+"
            android:textColor="@color/_679dfb"
            android:textSize="13sp"/>
    </LinearLayout>

    <!--验证码-->
    <RelativeLayout
        android:id="@+id/rlCode"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginLeft="@dimen/spacing28dp"
        android:background="@drawable/psw_white_one_shape"
        android:layout_marginRight="@dimen/spacing28dp"
        android:layout_marginTop="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/llCodeContent">

        <android.support.design.widget.TextInputEditText
            android:id="@+id/code"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/inputCode"
            android:layout_marginLeft="5dp"
            android:background="@null"
            android:textSize="@dimen/size14sp"
            android:textColor="@color/_333333"
            android:textColorHint="@color/_999999"
            android:layout_centerVertical="true"
            android:layout_marginRight="95dp"
            android:inputType="number"/>
        <TextView
            android:id="@+id/getCode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="5dp"
            android:textSize="11sp"
            android:textColor="@color/_26c68a"
            android:text="@string/getValidationCode"
            android:button="@drawable/checkbox_background_pass"
            android:checked="false"
            android:gravity="center"/>
    </RelativeLayout>

